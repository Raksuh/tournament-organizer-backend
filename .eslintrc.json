{
  "root": true,
  "env": {
    "es2021": true,
    "node": true,
    "jest": true
  },
  "extends": ["eslint:recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "ignorePatterns": [
    "node_modules/",
    "**/node_modules/",
    "package-lock.json",
    "**/package-lock.json",
    "package.json",
    "**/package.json",
    ".eslintrc.json",
    ".env",
    ".prettierrc"
  ],
  "rules": {
    "no-console": "warn"
    //  "no-console": "error"
  }
}
